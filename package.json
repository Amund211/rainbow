        "eslint-plugin-react-refresh": "^0.4.19",
        "glob": "^11.0.1",
        "globals": "^15.15.0",
        "prettier": "^3.5.0",
        "typescript": "~5.7.3",
        "typescript-eslint": "^8.24.0",
        "vite": "^6.1.0",
        "vite-plugin-react-click-to-component": "^3.1.1"
