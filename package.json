        "@tanstack/react-query": "^5.66.0",
        "@tanstack/react-router": "^1.102.5",
        "@tanstack/zod-adapter": "^1.102.5",
        "babel-plugin-react-compiler": "^19.0.0-beta-30d8a17-20250209",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "recharts": "^2.15.1",
        "zod": "^3.24.2"
